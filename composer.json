{
  "name": "codezero/cookie",
  "description": "Your friendly, furry cookie monster. Get and set cookies with ease.",
  "keywords": [
    "cookie",
    "encrypt",
    "php",
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Ivan Vermeyen",
      "email": "ivan@codezero.be"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "codezero/encrypter": "~2.0"
  },
  "require-dev": {
    "illuminate/cookie": ">=5.0",
    "illuminate/http": ">=5.0",
    "illuminate/support": ">=5.0",
    "phpunit/phpunit": "~8.0"
  },
  "scripts": {
    "test": "phpunit"
  },
  "autoload": {
    "psr-4": {
      "CodeZero\\Cookie\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CodeZero\\Cookie\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "CodeZero\\Cookie\\Laravel\\CookieServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
